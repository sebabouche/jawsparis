  "description": "Jaws.paris App",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  },
  "main": "server.babel.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebabouche/jawsparis.git"
  },
  "keywords": [
    "react",
    "3ree",
    "rethinkdb",
    "redux",
    "webpack",
    "uikit",
    "express",
    "node",
