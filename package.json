  "scripts": {
    "build:prod": "NODE_ENV=production webpack",
    "db-setup": "node dbSetup.babel.js",
    "lint": "eslint client server universal test server.js dbSetup.js",
    "start": "NODE_ENV=development node server.babel.js & node server.webpack.js",
    "start:prod": "NODE_ENV=production node server.babel.js",
    "test:3ree": "./node_modules/karma/bin/karma start",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "start:old": "node server",
    "build:staging": "webpack --config ./webpack.prod.config.js --staging --progress --colors",
    "build:production": "webpack --config ./webpack.prod.config.js --production --progress --colors",
    "clean": "rm -rf dist/*"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
